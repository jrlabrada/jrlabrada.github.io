      <div class="table-responsive">

        <table class="table table-striped table-sm">
            <% if(valor) {%>
                <form action="/modificar" method="POST" style="display:inline-flexbox; margin:20px; padding: 20px;   border-style: solid; width: 300px; margin-left: 40%;">
                 <input id="id" name="id" value="<%= valor.id %>" hidden> 
                  <div class="col-md-6">
                    <label for="date" class="form-label">Date</label>
                    <input type="date" class="form-control" id="date" name="date" value="<%= valor.fecha_entrada %>" style="width: 200px;">
                  </div>
                  <div class="col-md-6">
                      <label for="codigo" class="form-label">Código</label>
                      <input type="number" class="form-control" name="codigo" id="codigo" value="<%= valor.codigo %>" style="width: 100px;">
                    </div> 
                    <div class="col-md-4">
                      <label for="producto" class="form-label">Producto</label>
                      <select name="producto" id="producto" class="form-select" style="width: 250px;">
                        <option <%= valor.producto=='tacos cabeza rape' ? "selected" : '' %> value="Tacos Cabeza Rape">Tacos Cabeza Rape</option>
                        <option <%= valor.producto=='cabeza rape fresco' ? "selected" : '' %> value="Cabeza Rape Fresco">Cabeza Rape Fresco</option>
                        <option selected>Choose...</option>
                         
                        <option <%= valor.producto=='rabas de poton' ? "selected" : '' %> value="Rabas de Poton(PATATAS)">Rabas de Poton(PATATAS)</option>
                        <option <%= valor.producto=='rejos poton IQF (500-1000)' ? "selected" : '' %> value="Rejos Poton IQF (500-1000)">Rejos Poton IQF (500-1000)</option>
                        <option <%= valor.producto=='rejos poton IQF (1000-2000)' ? "selected" : '' %> value="Rejos Poton IQF (1000-2000)">Rejos Poton IQF (1000-2000)</option>
                        <option <%= valor.producto=='rodaja maruca' ? "selected" : '' %> value="Rodaja Maruca">Rodaja Maruca</option>
                        <option <%= valor.producto=='filete caballa' ? "selected" : '' %> value="Filete Caballa">Filete Caballa</option>
                        <option <%= valor.producto=='gallo (100-200)' ? "selected" : '' %> value="Gallo (100-200)">Gallo (100-200)</option>
                        <option <%= valor.producto=='cabeza rape bloque' ? "selected" : '' %> value="Cabeza Rape Bloque">Cabeza Rape Bloque</option>
                        <option <%= valor.producto=='rejo bandeja (500)' ? "selected" : '' %> value="Rejo Bandeja (500)">Rejo Bandeja (500)</option>
                        <option <%= valor.producto=='filete de lirio (40-60)' ? "selected" : '' %> value="Filete De Lirio (40-60)">Filete De Lirio (40-60)</option>
                        <option <%= valor.producto=='filete de merluza (15-252)' ? "selected" : '' %> value="Filete de Merluza (15-252)">Filete de Merluza (15-252)</option>
                         
                        <option selected>Filete de Lirio</option>
                        </select>
                    </div>
                    <div class="col-md-4">
                      <label for="almacen" class="form-label">Almacén</label>
                      <select id="almacen" name="almacen" class="form-select" style="width: 250px;">
                        <option <%= valor.almacen=='bergondo' ? "selected" : '' %> value="Bergondo">Bergondo</option>
                        <option <%= valor.almacen=='la coruñesa' ? "selected" : '' %> value="La Coruñesa">La Coruñesa</option>
                        </select>
                    </div>
                    <div class="col-md-6">
                      <label for="cajas" class="form-label">Cantidad de Cajas</label>
                      <input type="number" class="form-control" id="Ccajas" name="Ccajas" value="<%= valor.cajas %>"  style="width: 80px;">
                    </div> 
                    <div class="col-md-6">
                      <label for="kilos_totales" class="form-label">Kilos Totales</label>
                      <input type="number" class="form-control" id="Ktotales" value="<%= valor.kilos_totales %>" name="Ktotales" style="width: 100px;">
                    </div>
                    <div class="col-md-6">
                      <label for="glaseo" class="form-label">Glaseo</label>
                      <input type="number" class="form-control" id="glaseo" value="<%= valor.glaseo %>" name="glaseo" style="width: 60px;">
                    </div>
                    <div class="col-md-6">
                      <label for="lote" class="form-label">Lote</label>
                      <input type="text" class="form-control" id="lote" value="<%= valor.lote %>" name="lote" style="width: 120px;">
                    </div>
                    <div class="col-md-6">
                      <label for="cliente" class="form-label">Cliente</label>
                      <input type="text" class="form-control" name="cliente" id="cliente" value="<%= valor.cliente %>" style="width: 200px;">
                    </div> 
                    <div class="col-md-6">
                      <label for="no_palet" class="form-label">Numero de Palet</label>
                      <input type="number" class="form-control" name="Npalet" id="Npalet" value="<%= valor.no_palet %>" style="width: 120px;">
                    </div>
                  <div class="col-12" style="margin-top: 10px;">
                    <button type="submit" class="btn btn-primary">Guardar</button>
                    <a href="/" class="btn btn-secondary">Cancelar</a>
                  </div>
                </form>
                <% } %>
          
         
